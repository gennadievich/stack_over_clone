<%= form_for :question do |f| %>


    <p>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </p>

    <p>
        <%= f.label :description %>
        <%= f.text_area :description %>
    </p>


    <p>
        <%= f.label :tags %>
        <%= f.text_field :tages %>
    </p>

    <p>
        <%= f.submit %>
    </p>

<% end %>